<div class="container">
  <div class="row">
    <div class="col-6">
      <h4><会員情報></h4>
      <%= image_tag @end_user.get_profile_image, size:"200x200" %>
      <table class="table">
        <tbody>
          <tr>
            <th>ニックネーム</th>
            <td><%= @end_user.name %></td>
          </tr>
          <tr>
            <th>メールアドレス</th>
            <td><%= @end_user.email %></td>
          </tr>
          <tr>
            <th>自己紹介</th>
            <td><%= safe_join(@end_user.introduction.split("\n"),tag(:br)) %></td>
          </tr>
        </tbody>
      </table>
    </div>

    <div class="col-3 mt-5">
      <ul>
        <li><p><%= link_to "編集する", end_users_my_page_edit_path, class: "btn btn-outline-secondary btn-block" %></p></li>
        <li><p><%= link_to "投稿一覧をみる", end_users_plamos_path, class: "btn btn-outline-secondary btn-block" %></p></li>
        <li><p><%= link_to "レビュー一覧をみる", end_users_reviews_path, class: "btn btn-outline-secondary btn-block" %></p></li>
        <li><p><%= link_to "いいね一覧をみる", end_users_my_page_favorites_path, class: "btn btn-outline-secondary btn-block" %></p></li>
        <li><p><%= link_to "参考になった一覧をみる", end_users_my_page_usefuls_path, class: "btn btn-outline-secondary btn-block" %></p></li>
      </ul>
    </div>
  </div>
</div>