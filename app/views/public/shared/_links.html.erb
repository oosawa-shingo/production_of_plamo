<%- if controller_name != 'sessions' %>
  <%= link_to "こちら", new_session_path(resource_name), class: "btn btn-primary btn-block" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "こちら", new_registration_path(resource_name), class: "btn btn-success btn-block" %><br />
<% end %>
