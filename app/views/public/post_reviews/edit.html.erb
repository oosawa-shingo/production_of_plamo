<h1>編集画面</h1>

<%= form_with model: @post_review, url: post_review_path(@post_review.id), method: :patch do |f| %>

  <h4>画像</h4>
  <%= f.file_field :review_item_image, accept: "image/*" %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>商品名</h4>
  <%= f.text_field :review_item %>

  <h4>使ってみた感想.</h4>
  <%= f.text_area :feeling %>

  <%= f.submit '保存' %>

<% end %>